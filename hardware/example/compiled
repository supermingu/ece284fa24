    %flag_set/imm 4, 0;
    %store/vec4 v0x16ba8a0_0, 4, 1;
    %jmp T_0.1;
T_0.0 ;
    %pushi/vec4 1, 0, 1;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x16ba8a0_0, 4, 1;
